{
  "manifest_version": 2,
  "name": "RelaxnRetreat",
  "version": "1.0",
  "description": "A chrome extension that aims to assist users to relax and retreat!",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.min.js", "./main.js"]
    }
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": { "128": "./icon.png" },
  "permissions": ["tabs"]
}
